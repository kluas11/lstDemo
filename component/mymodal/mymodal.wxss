/* component/mymodal.wxss */
.modalbox {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 100;
}
.model_layout {
  width: 446rpx;
  height: 529rpx;
  border: 1epx solid #ccc;
  border-radius: 6rpx;
  background: #fff;
  margin: 200rpx auto 0;
  display: flex;
  align-items: center;
  flex-direction: column;
}
.model_img {
  padding-top: 100rpx;
  width: 212rpx;
  height: 170rpx;
}
.model_complete {
  margin-top: 40rpx;
  color: #000;
  font-size: 20rpx;
  font-weight: 500;
}
.model_foot_btn button {
  margin-top: 100rpx;
  font-size: 16rpx;
  color: #333;
  text-align: center;
  line-height: 40rpx;
  height: 40rpx;
  width: 140rpx;
  background: #ffffff;
  border: 1rpx solid #cccccc;
  border-radius: 4rpx;
}

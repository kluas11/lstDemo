.list_right {
  display: flex;
  width: 186rpx;
  height: 128rpx;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  border-left: 1rpx dashed #b5b5b5;
}

.list_right_price {
  font-size: 36rpx;
  color: #fa6958;
}

.list_right_originprice {
  margin: 6rpx 0;
  font-size: 16rpx;
  color: #999;
  text-decoration: line-through;
}

.list_right .cuopon_btn {
  width: 134rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #fff;
  background: #d94612;
  border-radius: 20rpx;
  height: 40rpx;
  line-height: 40rpx;
}

.list_right .cuopon_share {
  font-size: 28rpx;
  color:  #d94612;
  font-weight: 400;
  background: #ffffff;
  margin: 0;
  padding: 0;
  line-height: normal;
  margin-bottom: 14rpx;
  display: flex;
  align-items: center;
}
.share_img{
  width: 30rpx;
  height: 30rpx;
  margin-left: 6rpx;
}
/* 右边图片 */
.used_img {
  width: 156rpx;
  height: 119rpx;
}

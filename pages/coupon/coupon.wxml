<!--pages/coupon/coupon.wxml-->
<wxs module='cuopon' src='../../template/cuopon_cart/cuopon_cart.wxs' />
<view>
  <!-- 没有优惠券 -->
  <view wx:if="{{cuoponlist.length == 0}}" class="view-tint">
    <image src="/images/icon_empty.png"></image>
    <text>亲,没有数据</text>
  </view>
  <!-- 优惠券 -->
  <scroll-view scroll-y="true" class='cuopon_lists' wx:else>
    <include src="../../template/goods_coupon/goods_coupon.wxml" /> 
    <!-- 引用模板 -->
    <include src="../../template/cuopon_cart/cuopon_cart.wxml" />
  </scroll-view>
</view>
  <!-- <text bindtap="tabClick" data-index="1" class="{{tabClasss[1]}}">二级会员</text>
  <text bindtap="tabClick" data-index="2" class="{{tabClasss[2]}}">普通会员</text> -->
</view>


<view class="order-item" wx:for="{{orders}}" wx:key="">
  <view class="view-title">
    <image class="userinfo-avatar" src="{{item.head_pic}}" background-size="cover" bindtap="chooseImage"></image>
    <text class='userinfo-nick_name'>{{item.nick_name}}</text>
    <text class='userinfo-level_name'>{{item.level_name}}</text>
  </view>
</view>


<view wx:if="{{orders.length == 0}}" class="view-tint">
  <image src="/images/icon_empty.png"></image>
  <text>亲,没有数据</text>
</view>
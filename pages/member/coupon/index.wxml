<wxs module='cuopon' src='../../../template/cuopon_cart/cuopon_cart.wxs' />
<!-- 没有优惠券 -->
<view wx:if="{{cuoponlist.length == 0}}" class="view-tint">
  <image src="/images/icon_empty.png"></image>
  <text>亲,没有数据</text>
</view>
<view wx:else>
  <view class='header'>
    <view class='tab'>
      <text class='tab_list'>可使用(22)</text>
      <text class='tab_list'>已使用(12)</text>
      <text class='tab_list'>不可用(32)</text>
    </view>
    <view class='teb_page'>
      <text class='page_list'>全部类型</text>
      <text class='page_list'>优惠券</text>
      <text class='page_list'>商品券</text>
    </view>
  </view>
  <!-- 优惠券 -->
  <scroll-view scroll-y="true" class='cuopon_lists'>
    <!-- 引用模板 -->
    <include src="../../../template/cuopon_cart/cuopon_cart.wxml" />
  </scroll-view>
</view>
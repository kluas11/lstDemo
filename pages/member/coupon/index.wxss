@import '../../../template/cuopon_cart/cuopon_cart.wxss';
page{
  background: #e4e4e4;
}
.tab {
  height: 70rpx;
  font-size: 20rpx;
  color: #333;
  background: #f2f2f2;
  font-weight: 400;
  display: flex;
}

.tab_list {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}

.teb_page {
  height: 54rpx;
  background: #ffffff;
  font-size: 20rpx;
  color: #333333;
  font-weight: 400;
  display: flex;
}
.page_list{
  line-height: 54rpx;
  padding: 0 15rpx;
  margin-right: 30rpx;
}
.cuopon_lists {
  padding: 20rpx 40rpx;
  box-sizing: border-box;
  margin-top: 10rpx;
  background: #ffffff;
}

/* 没有数据 */

.view-tint {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin-top: 70rpx;
}

.view-tint image {
  width: 412rpx;
  height: 312rpx;
  margin-left: 100rpx;
}

.view-tint text {
  color: #888;
  font-size: 33rpx;
  margin-top: 30rpx;
}

@keyframes bg {
  0% {
    background-color: rgba(0, 0, 0, 0);
  }
  100% {
    background-color: rgba(0, 0, 0, 0.5);
  }
}
@keyframes up {
  0% {
    transform: translate3d(0, 665rpx, 0);
  }
  100% {
    transform: translate3d(0, 0, 0);
  }
}
page {
  height: 100%;
  background-color: #f2f2f2;
}
.container {
  width: 100%;
  font-size: 25rpx;
}
.container .img-wrapper {
  width: 100%;
  height: 248rpx;
  box-sizing: border-box;
  padding: 0 30rpx;
  background-color: #ffffff;
  border-bottom: 14rpx #f2f2f2 solid;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
}
.container .img-wrapper .top {
  width: 100%;
  height: 60rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.container .img-wrapper .top .edit {
  width: 111rpx;
  height: 44rpx;
  line-height: 44rpx;
  border: 1rpx #cccccc solid;
  border-radius: 5rpx;
  text-align: center;
}
.container .img-wrapper .list {
  flex: 1;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  overflow-x: auto;
}
.container .img-wrapper .list .list-img {
  flex: none;
  width: 139rpx;
  height: 139rpx;
  position: relative;
  margin-right: 20rpx;
}
.container .img-wrapper .list .list-img .img {
  flex: none;
  width: 100%;
  height: 100%;
}
.container .img-wrapper .list .list-img .text {
  flex: none;
  width: 100%;
  height: 39rpx;
  line-height: 39rpx;
  text-align: center;
  position: absolute;
  left: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.5);
  color: #ffffff;
}
.container .select-wrapper {
  background-color: #ffffff;
  box-sizing: border-box;
  border-bottom: 14rpx #f2f2f2 solid;
}
.container .select-wrapper .status {
  width: 100%;
  height: 83rpx;
  position: relative;
  padding: 0 30rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.container .select-wrapper .status::after {
  display: block;
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  border-top: 1rpx solid #f2f2f2;
  content: "";
  transform: scaleY(0.5);
}
.container .select-wrapper .status .right-arrow {
  width: 18rpx;
  height: 31rpx;
}
